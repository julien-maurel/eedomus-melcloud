      "device_id": "melcloud_onoff",
      "module_id": "41",
      "type": "http",
      "name_fr": "plugin.name - On/Off",
      "icon": "melcloud.png",
      "utilisation_id": "4",
      "parameters": {
        "value_type": "list",
        "ignore_errors": 3,
        "VAR1": "plugin.parameters.userpass",
        "VAR2": "plugin.parameters.ids",
        "RAW_URL": "http://localhost/script/?exec=melcloud.php&ids=[VAR2]&userpass=[VAR1]",
        "RAW_XPATH": "/root/Power",
        "POLLING": "5"
