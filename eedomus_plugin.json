  "parameters": [
    {
      "parameter": "userpass",
      "description": "email:password to connect to Melcloud",
      "xtype": "textfield",
      "default_value": "12345:67890",
      "field": "width:200, allowBlank:false"
    },
    {
      "parameter": "ids",
      "description": "idbuilding:iddevice",
      "xtype": "textfield",
      "default_value": "12345:67890",
      "field": "width:200, allowBlank:false"
    }
  ],
  "scripts": [
    {
      "name": "melcloud.php"
    }
  ],
  "devices": [
